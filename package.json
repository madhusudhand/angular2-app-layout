{
  "name": "angular2-boilerplate",
  "version": "1.0.0",
  "description": "Angular 2 boilerplate to kick start your new project",
  "readme": "README.md",
  "main": "Gruntfile.js",
  "scripts": {
    "start": "concurrent \"npm run lite\"",
    "dist": "concurrent \"npm run grunt:dist\" \"npm run lite\"",
    "lite": "lite-server",
    "tsc": "tsc",
    "grunt": "grunt",
    "grunt:dev": "grunt dev",
    "grunt:dist": "grunt dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madhusudhand/angular2-boilerplate"
  },
  "keywords": [
    "angular"
  ],
  "author": "Madhusudhan Dollu",
  "license": "MIT",
  "homepage": "https://github.com/madhusudhand/angular2-boilerplate/blob/master/README.md",
  "dependencies": {
    "angular2": "^2.0.0-beta.8",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.33.13",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "systemjs": "^0.19.23",
    "zone.js": "^0.5.15"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.0",
    "grunt-contrib-jade": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-sass": "^1.1.0",
    "grunt-ts": "^5.3.2",
    "lite-server": "^2.1.0"
  }
