{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "../build/",
    "mapRoot": "",
    "rootDir": "./src",
    "sourceRoot": "/",
    "declaration": false,
    "noEmitOnError": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false
  }
}
